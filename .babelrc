{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
      [
        "babel-plugin-transform-assets",
        {
          "extensions": ["css", "scss", "sass", "jpg", "jpeg", "png", "gif", "svg", "webp", "avif"],
          "name": "static/media/[name].[hash:8].[ext]"
        }
      ]
    ]
  }
